#!/bin/bash

# was eth0
INTERFACE=enp0s25

if [[ $EUID -ne 0 ]]; then
	sudo $0;
	exit
fi

macchanger $INTERFACE -m 2C:41:38:05:51:08
systemctl stop wicd
killall dhcpcd
killall wpa_supplicant
ip link set $INTERFACE up
wpa_supplicant -Dwired -i$INTERFACE -c/etc/wpa_supplicant/kolejnet.conf -B
dhcpcd $INTERFACE -t 120
